<body class="lightsnow">
    <div class="overlay">
        <div class="logo">
            <img src="https://appscyclone.com/templates/cyclone/images/apps-cyclone.png" alt="">
        </div>
        <div class="spin-container">
            <div class="canvas-container">
                <canvas id='canvas' width='600' height='600'>
                    Canvas not supported, use another browser.
                </canvas>
                <div class="foot-wheel">
                    <img src="./assets/images/foot.png" alt="">
                </div>
            </div>
            <button type="button" id="spin">
                <img src="./assets/images/gift.png" alt="">
                <!-- Click To Spin -->
            </button>
        </div>
    </div>




    <!-- Modal -->
    <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
        aria-hidden="true">
        <div class="modal-dialog" role="document">
            <button class="accept-action">Merry Christmas</button>
            <div class="modal-content">
                <div class="modal-body">
                    <div class="person-successful" style="font-family: 'Lobster', cursive;">
                        Nguyen Duc Tuan
                    </div>
                    <!-- <img src="http://pngimg.com/uploads/santa_claus/santa_claus_PNG38466.png" alt=""> -->
                </div>
            </div>
        </div>
    </div>

    <form class="del_form">
        <input type="hidden" type="text" name="id" value="39">
    </form>

    <div class="santa">
        <img style="width:100%" src="https://vignette.wikia.nocookie.net/icarly/images/5/51/Animated-santa-reindeer.gif/revision/latest?cb=20111125195748"
            alt="">
    </div>

    <div class="snow" style="display: block"></div>
    <div class="christmas-bg">
        <img  src="http://www.alc.edu/wp-content/uploads/2017/11/xmas-white-backgrounds-wallpapers.jpg" alt="">
        <!-- <img  src="./assets/images/1.jpg"alt=""> -->
        <!-- <img  src="./assets/images/2.jpg"alt=""> -->
    </div>

    <!-- img -->
    <!-- http://accessutilitiesuk.com/wp-content/uploads/2016/12/Xmas-BG.jpg -->
    <!-- https://www.riegsecker.com/assets/images/riegsecker/layout/bg-christmas.jpg -->

